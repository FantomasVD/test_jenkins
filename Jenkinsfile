pipeline {
  
  agent any
  
  stages {
  
    stage("Build") {
    
      steps {
      
        echo 'building the aplication...'
      
      }
    
    }
    
    stage("Test") {
    
      
      steps {
      
        echo 'testing the aplication...'
      
      }
    
    }
    
    stage("Deploy") {
    
      
      steps {
      
        echo 'deploy the aplication...'
      
      }
    
    }
  
  }
  
}
